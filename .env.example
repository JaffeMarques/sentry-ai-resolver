# Sentry Configuration (REQUIRED)
SENTRY_SOLVER_SENTRY_AUTH_TOKEN=your_sentry_token_here
SENTRY_SOLVER_SENTRY_ORGANIZATION_SLUG=your_org_slug

# Application Configuration (Optional - can be set via web interface)
SENTRY_SOLVER_CHECK_INTERVAL_MINUTES=30
SENTRY_SOLVER_MAX_ISSUES_PER_RUN=5

# Issue Filtering
SENTRY_SOLVER_ISSUE_MIN_SEVERITY=error
SENTRY_SOLVER_ISSUE_ENVIRONMENTS=production
SENTRY_SOLVER_ISSUE_MIN_OCCURRENCES=1
SENTRY_SOLVER_ISSUE_MAX_AGE_DAYS=30

# Git Configuration
SENTRY_SOLVER_GIT_BRANCH_PREFIX=sentry-fix
SENTRY_SOLVER_GIT_INCLUDE_ISSUE_ID=true
SENTRY_SOLVER_GIT_INCLUDE_TIMESTAMP=true
SENTRY_SOLVER_COMMIT_MESSAGE_PREFIX=fix
SENTRY_SOLVER_COMMIT_MESSAGE_FORMAT=conventional
SENTRY_SOLVER_GIT_AUTO_PUSH=true
SENTRY_SOLVER_GIT_CREATE_PR=false

# Logging
SENTRY_SOLVER_LOG_LEVEL=INFO